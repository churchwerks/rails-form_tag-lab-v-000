<h2>Student Form</h2>
<%= form_tag students_path do %>
  <label>First Name:</label><br>
  <%= text_field_tag id= 'first_name', :'student[first_name]' %><br>

  <label>Last Name:</label><br>
  <%= text_area_tag id= 'last_name', :'student[last_name]' %><br>

  <%= submit_tag "Submit Student" %>
<% end %>
